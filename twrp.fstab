# mount point	fstype		device			options (length for cryptfs encryption footer)
/boot			mtd		boot
/radio			mtd		radio
/cache			ext4	/dev/lvpool/cache      flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset;settingsstorage
/system			ext4	/dev/lvpool/system     flags=backup=1;canbewiped;wipeingui
/data			ext4	/dev/lvpool/data       flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/external_sd	auto	/dev/block/mmcblk1p1   flags=display="External SD Card";storage;wipeingui;removable
